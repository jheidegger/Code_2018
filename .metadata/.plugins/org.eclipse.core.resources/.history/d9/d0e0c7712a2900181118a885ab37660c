package Auton.Autos;

import Subsystem.Loop;

public class Auto {
	private static Loop autoLoop;
	public static String gameData;
	private static boolean firstTime = true;
	public Auto(Loop l)
	{
		autoLoop = l;
	}
	public static void setGameData(String gD)
	{
		gameData = gD;
	}
	public static void run()
	{
		if(firstTime)
		{
			autoLoop.onStart();
			firstTime = false;
		}
		else
		{
			autoLoop.onloop();
		}
	}	
}


